            </StackPanel>

            <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Hidden">
              <StackPanel x:Name="stackPanel3" Orientation="Vertical">
                <GroupBox
                  x:Name="groupBox_25"
                  Header="关于"
                  MouseEnter="GroupBox_MouseEnter"
                  Style="{StaticResource HiddenBorderGroupBoxStyle}">
                  <Border Style="{StaticResource CustomBorderStyle}">
                    <Grid>
                      <TextBox
                        x:Name="textBox"
                        MaxHeight="200"
                        Margin="10,10,10,10"
                        FontSize="16"
                        Style="{DynamicResource RoundTextBox2}"
                        Text="{DynamicResource TextBoxText2}"
                        TextWrapping="Wrap" />
                    </Grid>

                  </Border>
                </GroupBox>
                <GroupBox
                  x:Name="groupBox_27"
                  Header="全局设置说明"
                  MouseEnter="GroupBox_MouseEnter"
                  Style="{StaticResource HiddenBorderGroupBoxStyle}">
                  <Border Style="{StaticResource CustomBorderStyle}">
                    <Grid>
                      <StackPanel Orientation="Vertical">
                        <TextBox
                          Margin="10,10,10,10"
                          FontSize="16"
                          Style="{DynamicResource RoundTextBox2}"
                          Text="{DynamicResource TextBoxText1}"
                          TextWrapping="Wrap" />

                      </StackPanel>
                    </Grid>

                  </Border>
                </GroupBox>
              </StackPanel>
            </ScrollViewer>
          </Grid>


        </StackPanel>
      </StackPanel>

    </Grid>
  </Grid>
</Window>
