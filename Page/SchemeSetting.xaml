<class:BasePage
  x:Class="小科狗配置.Page.SchemeSetting"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:class="clr-namespace:小科狗配置.Class"
  xmlns:contorl="clr-namespace:小科狗配置.Contorl"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:小科狗配置"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  Title="SchemeSetting"
  FontSize="14"
  TextOptions.TextRenderingMode="ClearType"
  UseLayoutRounding="True"
  mc:Ignorable="d">

  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="35" />
      <RowDefinition Height="*" />
    </Grid.RowDefinitions>

    <StackPanel
      Margin="10,0,0,0"
      VerticalAlignment="Top"
      Orientation="Horizontal">
      <Button
        Height="24"
        Click="GetList_button_Click"
        Content="获取列表"
        Style="{DynamicResource RoundButton2}"
        ToolTip="从服务端获取方案列表" />
      <ComboBox
        x:Name="comboBox"
        Width="145"
        Height="24"
        Margin="5,0,0,0"
        Foreground="{DynamicResource TextForeground}"
        MouseEnter="ComboBox_MouseEnter"
        SelectionChanged="ComboBox_SelectionChanged"
        Style="{DynamicResource RoundComboBox}" />


      <Button
        x:Name="restor_Default_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Restor_default_button_Click"
        Content="重新读取"
        Style="{DynamicResource RoundButton2}"
        ToolTip="从服务端重新读取当前方案的配置" />

      <Button
        x:Name="loading_Templates_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Loading_templates_button_Click"
        Content="加载模板"
        Style="{DynamicResource RoundButton2}"
        ToolTip="从服务端读取默认的配置模板" />
      <Button
        x:Name="set_As_Default_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Set_as_default_Click"
        Content="设为默认"
        Style="{DynamicResource RoundButton2}"
        ToolTip="将当前方案设为所有进程默认方案" />

      <Button
        x:Name="apply_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Apply_button_Click"
        Content="应用修改"
        Style="{DynamicResource RoundButton2}"
        ToolTip="将当前方案内修改项更新到服务端" />
      <Button
        x:Name="apply_Save_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Apply_save_button_Click"
        Content="保存内存数据"
        Style="{DynamicResource RoundButton2}"
        ToolTip="点击后服务端将保存内存数据到到数据库" />
      <Button
        x:Name="apply_All_Button"
        Height="24"
        Margin="5,0,0,0"
        Click="Apply_All_button_Click"
        Content="更新内存数据"
        Style="{DynamicResource RoundButton2}"
        ToolTip="点击后服务端将从数据库重新读取配置" />

    </StackPanel>
